{
  "version": 3,
  "routes": [
    {
      "src": "/_app/.+",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/render"
    }
  ],
  "overrides": {
    "edit.html": {
      "path": "edit"
    },
    "make.html": {
      "path": "make"
    }
  }
}